{
    "minimum-stability": "stable",
    "prefer-stable": true,
    "name": "quantum-quill-coder/craft-stop-words",
    "description": "Enhance your Craft slugs effortlessly with our exclusive plugin! Automatically remove stop words from URLs for clean, SEO-friendly web addresses and an improved user experience.",
    "type": "craft-plugin",
    "license": "proprietary",
    "support": {
        "email": "fruite.simple0w@icloud.com",
        "issues": "https://github.com/quantumquillcoder/craft-stop-words/issues?state=open",
        "source": "https://github.com/quantumquillcoder/craft-stop-words",
        "docs": "https://github.com/quantumquillcoder/craft-stop-words",
        "rss": "https://github.com/quantumquillcoder/craft-stop-words/releases.atom"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.3.5"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "quantumquillcoder\\craftstopwords\\": "src/"
        }
    },
    
    "extra": {
        "handle": "stop-words",
        "name": "Stop words",
        "developer": "QuantumQuillCoder",
        "documentationUrl": "https://github.com/quantumquillcoder/craft-stop-words",
        "class": "quantumquillcoder\\craftstopwords\\StopWords"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
    
}

